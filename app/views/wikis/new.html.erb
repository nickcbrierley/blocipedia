<%= form_for @wiki do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>
   </div>
   <div class="form-group">   
      <%= f.label :body %>
      <%= f.text_area :body, rows: 20, class: 'form-control' %>
   </div>
   <div class="form-group">
      <%= f.submit "Save" %>
   </div>
 <% end %>
