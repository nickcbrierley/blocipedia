<div class="container-fluid">
<div class='row'>
    <div class='col'>
        <div class="jumbotron">
         <h1>Wikis <button><%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn' %></button></h1>
            <% @wikis.each do |wiki| %>
                <div class="row">
                  <div class="col-md-8">
                    <h4 class="media-heading">
                      <%= link_to wiki.title, wiki_path(wiki.id) %>
                    </h4>
                  </div>
                  <div class="col-md-4">
                   <%= link_to "Edit", edit_wiki_path(wiki.id), class: 'btn' %>
                   <%= link_to "Delete Wiki", wiki, method: :delete, class: 'btn', data: {confirm: 'Are you sure you want to delete this wiki?'} %>
                  </div>
                </div>
            <% end %>
         </div>
    </div>
 </div>
</div>